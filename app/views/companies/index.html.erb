<section id="team-list">
  <h2>Team List</h2>

  <% @companies_with_interviews.each do |classroom, companies| %>
    <h3><%= classroom.name %></h3>
    <ul>
      <% companies.each do |company| %>
        <li><%= link_to company.name, company_path(company.uuid) %></li>
      <% end %>
    </ul>
  <% end %>
</section>
